<MainLayout>
  <div data-type="wrapper">
    <div data-type="modal">
      <form action="">
      <div class="modal-header">
        <h2>Try Beta Version</h2>
        <svg
          xmlns="http://www.w3.org/2000/svg"
          width="16"
          height="17"
          viewBox="0 0 16 17"
          fill="none"
          data-type="modal__close-button"
        >
          <g
            id="Linear / Essentional, UI / Close Square"
            clip-path="url(#clip0_982_1534)"
          >
            <path
              id="Vector"
              d="M17.9999 -0.999919L-2 19M-2.00008 -1L17.9998 18.9999"
              stroke="#131313"
              stroke-width="1.5"
              stroke-linecap="round"></path>
          </g>
          <defs>
            <clipPath id="clip0_982_1534">
              <rect
                width="16"
                height="16"
                fill="white"
                transform="translate(0 0.5)"></rect>
            </clipPath>
          </defs>
        </svg>
      </div>
      <div class="modal-body">

          <label for="name">Name</label>
          <input
            type="text"
            class="full"
            name="name"
            id="name"
            placeholder="Name"
          />

        <label for="email" >E-mail</label>
        <input
          type="email"
          class="full"
          name="email"
          id="email"
          placeholder="E-mail"
          required
        />
      </div>
      <div class="checkbox">
        <input type="checkbox" required id="check" name="checkbox" />
        <label for="checkbox"
          >I have read and agree with the privacy policy and cookie policy*</label
        >
      </div>
      <button class="form-button" type="submit">Send Request</button>
    </form>
    </div>

  </div>
</MainLayout>

